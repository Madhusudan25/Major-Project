    <section id="carousel-section">
      <div
        id="carouselExampleIndicators"
        class="carousel slide"
        data-ride="carousel"
      >
        <ol class="carousel-indicators">
          <li
            data-target="#carouselExampleIndicators"
            data-slide-to="0"
            class="active"
          ></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img
              class="d-block w-100 carousal-image"
              src="/images/slider1.jpg"
              alt="To Do image"
            />
            <div class="carousel-caption d-none d-md-block">
              <h4>Mediblock</h4>
              <h5>Protect yourself by going one step ahead!</h5>
            </div>
          </div>
          <div class="carousel-item">
            <img
              class="d-block w-100 carousal-image"
              src="/images/slider2.jpg"
              alt="Portable image"
            />
            <div class="carousel-caption d-none d-md-block">
              <h4>Machine Learning</h4>
              <h5>Empowered by artificial brain!</h5>
            </div>
          </div>
          <div class="carousel-item">
            <img
              class="d-block w-100 carousal-image"
              src="/images/slider3.jpg"
              alt="Oauth image"
            />
            <div class="carousel-caption d-none d-md-block">
              <h4>Blockchain</h4>
              <h5>Never give away your data so easily!</h5>
            </div>
          </div>
          <div class="carousel-item">
            <img
              class="d-block w-100 carousal-image"
              src="/images/slider4.webp"
              alt="Students image"
            />
            <div class="carousel-caption d-none d-md-block">
              <h4>Encryption</h4>
              <h5>Hashing has your back!</h5>
            </div>
          </div>
        </div>
        <a
          class="carousel-control-prev"
          href="#carouselExampleIndicators"
          role="button"
          data-slide="prev"
        >
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a
          class="carousel-control-next"
          href="#carouselExampleIndicators"
          role="button"
          data-slide="next"
        >
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </section>

